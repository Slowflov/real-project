{
  "compilerOptions": {
    "allowJs": false,                
    "checkJs": false,                
    "jsx": "react-jsx",             
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,                 
    "skipLibCheck": true,           
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,         
    "strictNullChecks": true,      
    "strictFunctionTypes": true,   
    "strictBindCallApply": true,
    "declaration": true,           
    "noEmit": false                 
  },
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "include": ["src"]
}



